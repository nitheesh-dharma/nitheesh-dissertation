<EXCLUDED_CHAR MODIFIED="2011-10-18 09:57:55 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Peng-2006">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2011-10-18 09:57:55 +0100" MODIFIED_BY="[Empty name]">
