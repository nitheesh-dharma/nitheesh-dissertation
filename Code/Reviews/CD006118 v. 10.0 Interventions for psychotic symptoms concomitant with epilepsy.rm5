<CHARACTERISTICS_OF_INCLUDED_STUDIES MODIFIED="2015-12-01 10:13:54 +0000" MODIFIED_BY="[Empty name]" SORT_BY="STUDY" USER_DEF_1_ENABLED="NO" USER_DEF_2_ENABLED="NO" USER_DEF_3_ENABLED="NO">
<INCLUDED_CHAR MODIFIED="2015-12-01 10:13:48 +0000" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Thomas-2002">
<CHAR_METHODS MODIFIED="2015-12-01 10:13:31 +0000" MODIFIED_BY="[Empty name]">
<P>Double-blind, cross-over study</P>
</CHAR_METHODS>
<CHAR_PARTICIPANTS MODIFIED="2015-12-01 10:13:43 +0000" MODIFIED_BY="[Empty name]">
<P>16 people suffering from schizophrenia-like psychosis of epilepsy</P>
</CHAR_PARTICIPANTS>
<CHAR_INTERVENTIONS MODIFIED="2015-12-01 10:13:45 +0000" MODIFIED_BY="[Empty name]">
<P>Olanzapine (10 mg/day) compared with haloperidol (12 mg/day)</P>
</CHAR_INTERVENTIONS>
<CHAR_OUTCOMES MODIFIED="2015-12-01 10:13:48 +0000" MODIFIED_BY="[Empty name]">
<P>The participants were evaluated with electroencephalogram recording and Brief Psychiatric Rating Scale at baseline and after 3 and 6 months</P>
</CHAR_OUTCOMES>
<CHAR_NOTES MODIFIED="2008-08-05 22:47:20 +0100" MODIFIED_BY="[Empty name]"/>
</INCLUDED_CHAR>
