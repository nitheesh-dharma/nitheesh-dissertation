<STUDY DATA_SOURCE="PUB" ID="STD-Sturm-2003" MODIFIED="2013-03-12 13:34:57 +0000" MODIFIED_BY="[Empty name]" NAME="Sturm 2003" YEAR="2003">
<REFERENCE MODIFIED="2013-03-12 13:34:57 +0000" MODIFIED_BY="[Empty name]" PRIMARY="NO" TYPE="JOURNAL_ARTICLE">
