</CHAR_NOTES>
</INCLUDED_CHAR>
<INCLUDED_CHAR MODIFIED="2010-05-12 16:01:23 +0100" MODIFIED_BY="Hazel Fraser" STUDY_ID="STD-Uzuner-1995_x002f_180-mg">
