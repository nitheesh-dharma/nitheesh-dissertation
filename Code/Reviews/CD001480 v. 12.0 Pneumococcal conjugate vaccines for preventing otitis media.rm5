<APPENDIX_BODY MODIFIED="2013-02-08 10:57:19 +1000" MODIFIED_BY="[Empty name]">
<P>(mh:"otitis media" OR "otitis media" OR "Otite Mdia" OR mh:c09.218.705.663* OR aom) AND (mh:"Pneumococcal Vaccines" OR "Vacunas Neumococicas" OR "Vacinas Pneumoccicas" OR mh:"Vaccines, Conjugate" OR "Vacunas Conjugadas" OR "Vacinas Conjugadas" OR mh:"Bacterial Vaccines" OR "Vacunas Bacterianas" OR "Vacinas Bacterianas" OR "pneumococcal vaccine" OR "pneumococcal vaccines" OR "conjugate vaccines" OR "conjugate vaccine" OR pcv*) AND db:("LILACS") AND type_of_study:("clinical_trials")</P>
