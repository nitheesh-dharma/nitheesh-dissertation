<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2014-03-04 09:05:37 +0000" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Cabaluna-2012">
<DESCRIPTION>
