<STUDY DATA_SOURCE="PUB" ID="STD-Ammenwerth-2001" MODIFIED="2008-11-10 15:38:39 -0500" MODIFIED_BY="[Empty name]" NAME="Ammenwerth 2001" YEAR="2001">
<REFERENCE MODIFIED="2008-05-12 11:05:20 -0400" MODIFIED_BY="Christine Urquhart" PRIMARY="YES" TYPE="JOURNAL_ARTICLE">
