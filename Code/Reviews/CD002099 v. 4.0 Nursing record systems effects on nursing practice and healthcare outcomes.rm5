</CHAR_OUTCOMES>
<CHAR_USER_DEF_1 MODIFIED="2008-11-12 09:41:36 -0500" MODIFIED_BY="[Empty name]">
<P>Nuffield Provincial Hospitals Trust</P>
</CHAR_USER_DEF_1>
