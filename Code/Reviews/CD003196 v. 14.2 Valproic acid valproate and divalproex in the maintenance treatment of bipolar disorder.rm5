<INCLUDED_CHAR MODIFIED="2013-09-24 13:16:44 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Kemp-2009">
<CHAR_METHODS MODIFIED="2013-09-24 13:06:11 +0100" MODIFIED_BY="Keith S Reid">
<P>Randomised double-blind placebo-controlled parallel-group trial</P>
</CHAR_METHODS>
<CHAR_PARTICIPANTS MODIFIED="2013-09-24 13:16:44 +0100" MODIFIED_BY="[Empty name]">
