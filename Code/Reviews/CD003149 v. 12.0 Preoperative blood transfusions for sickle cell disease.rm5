<QUALITY_ITEM_DATA>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-08.01" MODIFIED="2015-11-11 11:16:51 +0000" MODIFIED_BY="Tracey Remmington" RESULT="UNKNOWN" STUDY_ID="STD-Al_x002d_Jaouni-2006">
