<QUALITY_ITEM_DATA_ENTRY MODIFIED="2016-04-04 14:23:12 +0100" MODIFIED_BY="Tracey Remmington" RESULT="NO" STUDY_ID="STD-Al_x002d_Jaouni-2006">
<DESCRIPTION>
