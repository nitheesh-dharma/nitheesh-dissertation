<INCLUDED_CHAR MODIFIED="2014-08-19 17:37:21 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-ANCHOR-2006">
<CHAR_METHODS MODIFIED="2014-08-18 12:59:42 +0100" MODIFIED_BY="[Empty name]">
<P>
