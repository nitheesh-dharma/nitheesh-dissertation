<QUALITY_ITEM_DATA_ENTRY MODIFIED="2013-10-03 09:36:57 +0100" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Leslie-2008">
<DESCRIPTION>
