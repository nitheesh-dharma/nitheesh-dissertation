<QUALITY_ITEM_DATA_ENTRY MODIFIED="2009-02-24 02:16:47 +0000" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Ball-2004">
<DESCRIPTION>
<OL>
