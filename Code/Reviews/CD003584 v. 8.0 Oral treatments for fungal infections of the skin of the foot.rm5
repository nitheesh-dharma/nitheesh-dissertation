<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2012-09-10 16:08:27 +0100" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Svejgaard-1998">
