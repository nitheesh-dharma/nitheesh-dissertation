<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-12.01" MODIFIED="2012-02-09 10:11:29 +0000" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Morey-2010">
