<STUDY DATA_SOURCE="PUB" ID="STD-Ibrahiem-1988" MODIFIED="2010-02-04 09:29:51 -0600" MODIFIED_BY="James Tacklind" NAME="Ibrahiem 1988" YEAR="1988">
