<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-07.01" MODIFIED="2012-11-12 12:18:42 +0000" MODIFIED_BY="Dawn Dobson" RESULT="NO" STUDY_ID="STD-Heber-2003">
