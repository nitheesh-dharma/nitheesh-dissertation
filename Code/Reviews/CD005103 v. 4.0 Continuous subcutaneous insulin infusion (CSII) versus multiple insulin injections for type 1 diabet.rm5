<OTHER_OUTCOME COLS="4" ID="CMP-001.19" MODIFIED="2009-09-07 06:48:02 +0200" MODIFIED_BY="[Empty name]" NO="19" ROWS_PER_STUDY="1" SORT_BY="STUDY" STUDIES="15" SUBGROUPS="NO">
<NAME>Quality of life</NAME>
<TR>
