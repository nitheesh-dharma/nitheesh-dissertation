<EXCLUDED_CHAR MODIFIED="2011-02-18 12:36:45 +0000" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Peng-2006">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2011-02-18 12:36:45 +0000" MODIFIED_BY="[Empty name]">
