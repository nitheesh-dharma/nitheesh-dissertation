<EXCLUDED_CHAR MODIFIED="2010-10-07 16:46:25 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Nienhuis-1976">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2010-10-07 16:46:25 +0100" MODIFIED_BY="[Empty name]">
