<STUDY DATA_SOURCE="PUB" ID="STD-Tandon-2010" MODIFIED="2013-07-31 13:47:15 -0400" MODIFIED_BY="Mosarrat J Qureshi" NAME="Tandon 2010" YEAR="2007">
