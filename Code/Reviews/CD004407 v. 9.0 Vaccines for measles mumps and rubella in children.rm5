<TABLE COLS="10" ROWS="6">
<TR>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Study and design</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Outcome</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Population</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Definition</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Findings</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>MMR type</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Risk time</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Results</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
<P>Risk of bias</P>
</TH>
<TH ALIGN="LEFT" VALIGN="BOTTOM">
