<STUDY DATA_SOURCE="PUB" ID="STD-Laxdal-1970" MODIFIED="2012-02-22 17:10:14 +1000" MODIFIED_BY="Roderick P. Venekamp" NAME="Laxdal 1970" YEAR="1970">
