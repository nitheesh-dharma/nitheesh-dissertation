<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2013-06-18 22:11:14 +0200" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Fischer-1999">
