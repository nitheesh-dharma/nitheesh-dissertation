<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2013-05-13 11:21:29 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Nelson-1980">
