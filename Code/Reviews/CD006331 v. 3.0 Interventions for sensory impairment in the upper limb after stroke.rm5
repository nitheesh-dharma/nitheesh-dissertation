<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2010-03-24 11:04:35 +0000" MODIFIED_BY="Hazel Fraser" RESULT="YES" STUDY_ID="STD-Feys-1998">
