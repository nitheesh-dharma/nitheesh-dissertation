<EXCLUDED_CHAR MODIFIED="2017-01-04 19:03:01 -0500" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Dellinger-2005">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2017-01-04 19:03:01 -0500" MODIFIED_BY="[Empty name]">
