<STUDY DATA_SOURCE="PUB" ID="STD-Barakat-2005-EGY" MODIFIED="2012-02-28 15:19:36 +0000" MODIFIED_BY="[Empty name]" NAME="Barakat 2005 EGY" YEAR="2005">
