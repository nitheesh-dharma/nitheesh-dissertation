<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2013-09-24 10:23:25 +0100" MODIFIED_BY="Nikki Jahnke" RESULT="NO" STUDY_ID="STD-Albinni-2004">
<DESCRIPTION>
