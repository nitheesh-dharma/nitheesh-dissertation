<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2010-02-10 11:12:07 +0100" MODIFIED_BY="Sera Tort" RESULT="NO" STUDY_ID="STD-Johnstone-2002">
