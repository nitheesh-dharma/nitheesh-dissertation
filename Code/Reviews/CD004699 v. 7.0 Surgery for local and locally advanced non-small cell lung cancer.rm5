<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2010-01-22 09:20:22 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Izbicki-1998">
