<EXCLUDED_CHAR MODIFIED="2011-11-13 19:26:49 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Ryan-1967">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2011-11-13 19:26:49 +0100" MODIFIED_BY="[Empty name]">
