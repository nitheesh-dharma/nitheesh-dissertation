<QUALITY_ITEM_DATA_ENTRY MODIFIED="2017-05-22 15:35:40 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-L_x00f3_pez-2005">
<DESCRIPTION>
