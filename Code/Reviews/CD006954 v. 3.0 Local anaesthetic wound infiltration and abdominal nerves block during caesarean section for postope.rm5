<EXCLUDED_CHAR MODIFIED="2008-06-23 16:33:08 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Lipscomb-2002">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2008-06-23 16:33:08 +0100" MODIFIED_BY="[Empty name]">
