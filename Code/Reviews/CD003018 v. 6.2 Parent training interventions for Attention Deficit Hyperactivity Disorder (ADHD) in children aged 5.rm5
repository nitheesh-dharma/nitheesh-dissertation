<QUALITY_ITEM_DATA_ENTRY MODIFIED="2010-12-30 16:44:12 +0000" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Fallone-1998">
<DESCRIPTION>
