<INCLUDED_CHAR MODIFIED="2013-02-26 10:42:53 +0000" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Locke-1986">
<CHAR_METHODS>
<P>Random assignment</P>
</CHAR_METHODS>
<CHAR_PARTICIPANTS MODIFIED="2013-02-26 10:42:53 +0000" MODIFIED_BY="Anne Lawson">
<P>53 juvenile delinquents ages 14-19 years on probation from 3 Kansas counties, 65% white, 100% male</P>
</CHAR_PARTICIPANTS>
<CHAR_INTERVENTIONS>
