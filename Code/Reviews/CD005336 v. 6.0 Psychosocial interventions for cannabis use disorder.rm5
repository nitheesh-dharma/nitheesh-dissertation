<QUALITY_ITEM_DATA_ENTRY MODIFIED="2016-04-28 12:30:52 +0200" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Fischer-2012">
<DESCRIPTION>
