<QUALITY_ITEM_DATA_ENTRY MODIFIED="2016-04-28 12:30:53 +0200" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Hoch-2012">
<DESCRIPTION>
