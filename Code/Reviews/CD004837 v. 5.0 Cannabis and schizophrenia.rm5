<STUDY DATA_SOURCE="SOUGHT" ID="STD-Hjorthoj-2013" MODIFIED="2014-10-01 12:04:46 +0100" MODIFIED_BY="[Empty name]" NAME="Hjorthoj 2013" YEAR="2013">
<REFERENCE MODIFIED="2014-10-01 12:04:46 +0100" MODIFIED_BY="[Empty name]" PRIMARY="NO" TYPE="JOURNAL_ARTICLE">
