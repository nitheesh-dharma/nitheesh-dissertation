<EXCLUDED_CHAR MODIFIED="2015-09-14 13:22:25 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Brown-2007">
<CHAR_REASON_FOR_EXCLUSION MODIFIED="2015-09-14 13:22:25 +0100" MODIFIED_BY="[Empty name]">
