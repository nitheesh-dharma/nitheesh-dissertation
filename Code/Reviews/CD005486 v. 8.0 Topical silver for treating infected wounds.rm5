<REFERENCE MODIFIED="2008-08-07 14:29:17 +0100" MODIFIED_BY="[Empty name]" NOTES="&lt;p&gt;// HS // /Poster/&lt;/p&gt;" NOTES_MODIFIED="2008-08-07 14:29:17 +0100" NOTES_MODIFIED_BY="[Empty name]" PRIMARY="NO" TYPE="CONFERENCE_PROC">
