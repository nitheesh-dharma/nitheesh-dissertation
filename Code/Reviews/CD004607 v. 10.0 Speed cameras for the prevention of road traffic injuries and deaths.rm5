<QUALITY_ITEMS MODIFIED="2010-06-11 03:58:57 +0100" MODIFIED_BY="[Empty name]">
<QUALITY_ITEM CORE_ITEM="YES" ID="QIT-01" LEVEL="STUDY" NO="1">
