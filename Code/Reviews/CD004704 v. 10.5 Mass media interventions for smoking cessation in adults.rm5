<OTHER_OUTCOME COLS="2" ID="CMP-001.02" MODIFIED="2017-09-20 15:03:55 +0100" MODIFIED_BY="Jonathan Livingstone-Banks" NO="2" ROWS_PER_STUDY="1" SORT_BY="STUDY" STUDIES="11" SUBGROUPS="NO">
