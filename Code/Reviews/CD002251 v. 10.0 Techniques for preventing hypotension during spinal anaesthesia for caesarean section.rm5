<AWAITING_CHAR MODIFIED="2017-07-18 08:55:14 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-Boswell-2008">
<CHAR_METHODS MODIFIED="2017-05-12 12:37:10 +0100" MODIFIED_BY="[Empty name]">
<P>RCT</P>
