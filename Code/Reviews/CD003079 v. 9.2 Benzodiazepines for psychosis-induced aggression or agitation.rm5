<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2017-11-24 14:36:57 +0000" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Lorazepam-1998_x002c_-USA">
