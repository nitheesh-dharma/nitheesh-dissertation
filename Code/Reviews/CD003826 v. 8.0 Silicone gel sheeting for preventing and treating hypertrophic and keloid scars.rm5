<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-07.01" MODIFIED="2011-10-31 15:13:39 +0000" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Colom-Majan-2006">
<DESCRIPTION>
