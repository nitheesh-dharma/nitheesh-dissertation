<STUDY DATA_SOURCE="PUB" ID="STD-Cavicchioli-2007" MODIFIED="2008-07-14 15:15:34 +0100" MODIFIED_BY="[Empty name]" NAME="Cavicchioli 2007" YEAR="2007">
