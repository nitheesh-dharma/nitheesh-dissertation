<QUALITY_ITEM_DATA_ENTRY MODIFIED="2012-03-25 10:31:50 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Cohen-1998">
<DESCRIPTION>
