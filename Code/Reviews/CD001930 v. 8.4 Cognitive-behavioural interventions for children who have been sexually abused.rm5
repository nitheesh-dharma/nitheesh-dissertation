</QUALITY_ITEM_DATA_ENTRY>
<QUALITY_ITEM_DATA_ENTRY MODIFIED="2012-03-25 10:33:44 +0100" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Cohen-2004">
