<QUALITY_ITEM_DATA_ENTRY MODIFIED="2010-01-27 12:39:47 +0000" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Konikoff-2006">
<DESCRIPTION>
