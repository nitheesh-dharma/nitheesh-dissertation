</QUALITY_ITEM_DATA_ENTRY>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-07.02" MODIFIED="2013-11-14 15:04:08 +0200" MODIFIED_BY="Babs J.M.  Faber" RESULT="NO" STUDY_ID="STD-Schene-2006">
