<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.02" MODIFIED="2017-01-06 13:28:34 +0000" MODIFIED_BY="Kate Cahill" RESULT="UNKNOWN" STUDY_ID="STD-Wapner-1995">
