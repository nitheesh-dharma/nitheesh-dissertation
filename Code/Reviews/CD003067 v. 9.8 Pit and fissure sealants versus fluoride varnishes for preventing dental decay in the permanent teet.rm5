<QUALITY_ITEM_DATA_ENTRY MODIFIED="2015-12-11 11:25:26 +0000" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Splieth-2001">
<DESCRIPTION>
