<IDENTIFIERS MODIFIED="2016-05-20 14:22:32 +0200" MODIFIED_BY="[Empty name]"><IDENTIFIER OTHERTYPE="CRS-ID" TYPE="OTHER" VALUE="6800100000089347"/><IDENTIFIER MODIFIED="2016-05-20 14:22:32 +0200" MODIFIED_BY="[Empty name]" TYPE="DOI" VALUE="10.1002/14651858.CD003328.pub2"/><IDENTIFIER OTHERTYPE="JC--NLM" TYPE="OTHER" VALUE="Journal ID:100909747"/></IDENTIFIERS>
