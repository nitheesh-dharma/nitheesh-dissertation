<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2015-11-06 13:33:57 +0000" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Sullivan-1991">
<DESCRIPTION>
