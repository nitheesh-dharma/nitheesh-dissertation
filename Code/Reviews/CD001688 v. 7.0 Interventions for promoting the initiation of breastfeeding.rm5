<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-12.01" MODIFIED="2016-10-27 23:47:51 +0100" MODIFIED_BY="Clare Dooley" RESULT="UNKNOWN" STUDY_ID="STD-Kirkwood-2013">
