</QUALITY_ITEM_DATA_ENTRY>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-12.01" MODIFIED="2016-08-02 11:56:31 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Kellams-2016">
