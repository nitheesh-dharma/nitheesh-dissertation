<QUALITY_ITEM_DATA_ENTRY MODIFIED="2012-11-01 20:35:08 +0000" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Saul-1982">
<DESCRIPTION>
