</QUALITY_ITEM_DATA_ENTRY>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2012-01-11 15:22:48 +1100" MODIFIED_BY="Melina Willson" RESULT="NO" STUDY_ID="STD-Matloff-2006">
