<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2013-04-07 12:36:36 +0100" MODIFIED_BY="[Empty name]" RESULT="YES" STUDY_ID="STD-Kushner-1998a">
