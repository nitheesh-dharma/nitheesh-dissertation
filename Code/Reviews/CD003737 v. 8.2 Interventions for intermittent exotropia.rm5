<QUALITY_ITEM_DATA>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2013-04-07 12:36:26 +0100" MODIFIED_BY="Anupa Shah" RESULT="NO" STUDY_ID="STD-Kushner-1998a">
