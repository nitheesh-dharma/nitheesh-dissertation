<QUALITY_ITEM_DATA>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-04.01" MODIFIED="2010-02-08 10:29:06 +0000" MODIFIED_BY="Helen HG Handoll" RESULT="UNKNOWN" STUDY_ID="STD-Bergman-1982">
