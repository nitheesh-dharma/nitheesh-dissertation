<QUALITY_ITEM_DATA_ENTRY MODIFIED="2014-05-05 11:14:35 +0200" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Karamanakos-2008">
<DESCRIPTION>
