<CHAR_METHODS MODIFIED="2013-02-01 10:39:33 +0000" MODIFIED_BY="[Empty name]">
<P>Randomised, multicentre, open-label, controlled phase III, treatment trial</P>
</CHAR_METHODS>
<CHAR_PARTICIPANTS MODIFIED="2013-01-24 10:36:30 +0000" MODIFIED_BY="[Empty name]">
