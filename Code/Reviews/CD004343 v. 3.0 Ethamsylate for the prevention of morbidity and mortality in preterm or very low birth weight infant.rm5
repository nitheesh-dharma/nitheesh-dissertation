<QUALITY_ITEM_DATA>
<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2009-11-03 11:26:34 -0500" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Amato-1993">
