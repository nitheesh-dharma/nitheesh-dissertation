<STUDY DATA_SOURCE="PUB" ID="STD-Adam-1996" MODIFIED="2011-11-24 11:28:55 +1000" MODIFIED_BY="[Empty name]" NAME="Adam 1996" YEAR="">
<REFERENCE MODIFIED="2011-11-24 11:28:55 +1000" MODIFIED_BY="[Empty name]" PRIMARY="YES" TYPE="JOURNAL_ARTICLE">
