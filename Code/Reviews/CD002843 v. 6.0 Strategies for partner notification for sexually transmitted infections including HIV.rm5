<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-08.01" MODIFIED="2013-06-18 12:52:05 -0500" MODIFIED_BY="[Empty name]" RESULT="NO" STUDY_ID="STD-Moyo-2002">
