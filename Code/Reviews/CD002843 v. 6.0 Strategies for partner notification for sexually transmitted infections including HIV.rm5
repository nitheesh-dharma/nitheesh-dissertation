<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-08.01" MODIFIED="2013-06-18 12:05:11 -0500" MODIFIED_BY="[Empty name]" RESULT="UNKNOWN" STUDY_ID="STD-Schwebke-2010">
