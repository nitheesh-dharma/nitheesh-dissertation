<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-08.02" MODIFIED="2016-10-04 14:26:09 +0100" MODIFIED_BY="Tracey Remmington" RESULT="NO" STUDY_ID="STD-STOP-1998">
