<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2013-12-02 23:22:31 +0000" MODIFIED_BY="Filip Eftimov" RESULT="YES" STUDY_ID="STD-Thompson-1996">
