<CHARACTERISTICS_OF_AWAITING_STUDIES MODIFIED="2014-09-03 17:34:52 +0100" MODIFIED_BY="[Empty name]" SORT_BY="STUDY" USER_DEF_1_ENABLED="NO" USER_DEF_2_ENABLED="NO" USER_DEF_3_ENABLED="NO">
<AWAITING_CHAR MODIFIED="2014-09-03 17:34:23 +0100" MODIFIED_BY="[Empty name]" STUDY_ID="STD-NCT00744328">
<CHAR_METHODS MODIFIED="2014-08-04 15:20:13 +0100" MODIFIED_BY="[Empty name]">
<P>8-week, double-blind, placebo-controlled randomised controlled trial</P>
</CHAR_METHODS>
<CHAR_PARTICIPANTS MODIFIED="2014-08-04 15:18:33 +0100" MODIFIED_BY="[Empty name]">
<P>85 women</P>
