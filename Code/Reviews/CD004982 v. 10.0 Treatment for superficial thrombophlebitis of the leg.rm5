<INCLUDED_STUDIES MODIFIED="2018-02-14 13:55:07 +0000" MODIFIED_BY="[Empty name]">
<STUDY DATA_SOURCE="PUB" ID="STD-Andreozzi-1996" NAME="Andreozzi 1996" YEAR="1996">
