<QUALITY_ITEM_DATA_ENTRY GROUP_ID="QIG-03.01" MODIFIED="2012-11-27 14:34:57 +0000" MODIFIED_BY="Jennifer R Evans" RESULT="NO" STUDY_ID="STD-Lebuisson-1986">
